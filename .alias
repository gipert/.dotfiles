alias ls='ls --color=auto -F'
alias l='ls --color=auto -F -lh'
alias ll='ls --color=auto -F -lah'
alias grep='grep --color=auto'
alias root='root -l'
alias cd='cd -P'
alias pwd='pwd -P'
alias y='sudo yum'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../../'
alias .....='cd ../../../..'
alias untar='tar xf'
alias dotsync='cd ~/.dotfiles && git pull && cd - &> /dev/null'

[[ -f ~/.alias_bis ]] && source ~/.alias_bis
